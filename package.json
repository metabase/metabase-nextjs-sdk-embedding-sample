{
  "name": "metabase-nextjs-sdk-embedding-sample",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "cypress": "^14.0.3",
    "dotenv-cli": "^8.0.0"
  },
  "scripts": {
    "start": "npm run docker:up",
    "docker:up": "docker compose --env-file .env.docker up",
    "docker:down": "docker compose --env-file .env.docker down --rmi all --volumes",
    "cypress:open": "dotenv -e .env.docker -- npx cypress open --config-file ./e2e/support/cypress.config.js --e2e",
    "cypress:run": "dotenv -e .env.docker -- npx cypress run --config-file ./e2e/support/cypress.config.js"
  }
}
